    "child_process": "^1.0.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "exceljs": "^4.3.0",
    "formidable": "^3.5.0",
